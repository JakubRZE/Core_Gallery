<script>
    var width = $(window).width();
    if ((width > 960) && (@Model.Count % 4 === 1)) {
        $("img:last").css('max-height', '250px');
    }
    else {
        $("img:last").css('max-height', '105px');
    }


    $(window).resize(function () {
        width = $(window).width();
        if ((width > 960) && (@Model.Count % 4 === 1)) {
            $("img:last").css('max-height', '250px');
        }
        else {
            $("img:last").css('max-height', '105px');
        }
    });
</script>